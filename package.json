{
  "version": "0.1.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-rowe/brnstn"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4
  },
  "name": "brnstn",
  "author": "Daniel Rowe",
  "module": "dist/brnstn.esm.js",
  "size-limit": [
    {
      "path": "dist/brnstn.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/brnstn.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/keymirror": "^0.1.1",
    "@types/sinon": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "husky": "^5.0.6",
    "pinst": "^2.1.1",
    "sinon": "^9.2.2",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "keymirror": "^0.1.1"
  }
}
